{
  "name": "Amazon India Search",
  "description": "Tab search Amazon India",
  "manifest_version": 2,
  "version": "0.1",
  "browser_specific_settings": {
    "gecko": {
      "id": "amazon_india@varsedile"
    }
  },
  "icons": {
		"32": "favicon.ico.png"
	},
  "hidden": true,
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "@amazon",
      "name": "Amazon India Search",
      "search_url": "https://www.amazon.in/s",
      "search_url_get_params": "k={searchTerms}"
    }
  }
}
